{
  "name": "i18next-manager",
  "version": "1.0.0",
  "description": "Import/Export capability for i18next translation files",
  "main": "./lib/index.js",
  "scripts": {
    "start": "tsc --watch",
    "dev:import": "ts-node src/index.ts -i -t ./locales/ -o ./i18next-assets/Import.xlsx",
    "dev:export": "ts-node src/index.ts -e -t ./locales/ -o ./i18next-assets/Export.xlsx",
    "build": "tsc --build",
    "lint": "eslint --ext \"ts\" -c ./.eslintrc.json ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Bohdan Heptinh",
  "license": "ISC",
  "dependencies": {
    "commander": "^4.0.0",
    "exceljs": "^3.3.1",
    "lodash": "^4.17.15",
    "shelljs": "^0.8.3"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.6",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  },
  "bin": {
    "i18next-manager": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bheptinh/i18next-manager.git"
  },
  "bugs": {
    "url": "https://github.com/bheptinh/i18next-manager/issues"
  },
  "homepage": "https://github.com/bheptinh/i18next-manager#readme"
}
